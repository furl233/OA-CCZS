A,cALA;AAMA,IAAA,iBAAA,EAAA,iBANA;AAOA,IAAA,oBAAA,EAAA,oBAPA;AAQA,IAAA,UAAA,EAAA,UARA;AASA,IAAA,kBAAA,EAAA;AATA,GAtDA;AAiEA,EAAA,OAAA,EAAA;AACA,IAAA,IAAA,EAAA,IADA;AACA,IAAA,MAAA,EAAA,MADA;AAEA,IAAA,WAAA,EAAA,WAFA;AAEA,IAAA,WAAA,EAAA,WAFA;AAEA,IAAA,WAAA,EAAA,WAFA;AAEA,IAAA,WAAA,EAAA,WAFA;AAEA,IAAA,WAAA,EAAA,WAFA;AAGA,IAAA,iBAAA,EAAA,iBAHA;AAGA,IAAA,cAAA,EAAA,cAHA;AAGA,IAAA,aAAA,EAAA,aAHA;AAIA,IAAA,OAJA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,kBAIA;AACA,MAAA,OAAA,CAAA,GAAA,CACA,KAAA,IADA,EACA,KAAA,WADA,EACA,KAAA,QADA,EAEA,KAAA,cAFA,EAEA,KAAA,KAFA,EAEA,KAAA,QAFA,EAGA,KAAA,SAHA,EAGA,KAAA,YAHA,EAGA,KAAA,IAHA,EAIA,KAAA,OAJA,EAIA,KAAA,cAJA,EAIA,KAAA,MAJA,EAKA,KAAA,cALA,EAKA,KAAA,WALA,EAKA,KAAA,UALA,EAKA,KAAA,SALA;AAOA,KAZA;AAaA,IAAA,KAbA,iBAaA,aAbA,EAaA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,aAAA;AACA,KAfA;AAgBA,IAAA,KAhBA,iBAgBA,eAhBA,EAgBA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,eAAA;AACA,KAlBA;AAmBA,IAAA,QAnBA,sBAmBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KArBA;AAsBA;AACA,IAAA,QAvBA,oBAuBA,KAvBA,EAuBA;AACA,UAAA,KAAA,EAAA;AACA,aAAA,aAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,aAAA;AACA,YAAA,GAAA,GAAA,IAAA;;AACA,YAAA,MAAA,CAAA,eAAA,IAAA,SAAA,EAAA;AACA,UAAA,GAAA,GAAA,MAAA,CAAA,eAAA,CAAA,KAAA,CAAA;AACA,SAFA,MAEA,IAAA,MAAA,CAAA,SAAA,IAAA,SAAA,EAAA;AACA,UAAA,GAAA,GAAA,MAAA,CAAA,SAAA,CAAA,eAAA,CAAA,KAAA,CAAA;AACA,SAFA,MAEA,IAAA,MAAA,CAAA,GAAA,IAAA,SAAA,EAAA;AACA,UAAA,GAAA,GAAA,MAAA,CAAA,GAAA,CAAA,eAAA,CAAA,KAAA,CAAA;AACA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,aAAA,UAAA,GAAA,GAAA;AACA,OAbA,MAaA;AAAA,aAAA,UAAA,GAAA,EAAA,EAAA,KAAA,aAAA,GAAA,IAAA;AAAA;AACA,KAtCA;AAuCA,IAAA,iBAvCA,+BAuCA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,aAAA;AACA;AAzCA,GAjEA;AA4GA,EAAA,KAAA,EAAA;AACA;AACA,IAAA,IAFA,gBAEA,GAFA,EAEA;AAAA;;AACA,MAAA,GAAA,IAAA,UAAA,CAAA;AAAA,eAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,YAAA,GAAA,MAAA;AAAA,OAAA,CAAA;AACA,KAJA;AAKA,IAAA,KALA,iBAKA,GALA,EAKA;AAAA;;AACA,MAAA,GAAA,IAAA,UAAA,CAAA;AAAA,eAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,YAAA,GAAA,MAAA;AAAA,OAAA,CAAA;AACA,KAPA;AAQA,IAAA,KARA,iBAQA,GARA,EAQA;AAAA;;AACA,MAAA,GAAA,IAAA,UAAA,CAAA;AAAA,eAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,YAAA,GAAA,MAAA;AAAA,OAAA,CAAA;AACA;AAVA;AA5GA,CAAA","sourcesContent":["<template>\n  <v-container>\n    <v-layout row warp justify-center>\n      <v-flex xs12 md6>\n        <v-card>\n          <v-toolbar flat color=\"indigo\">\n            <v-icon class=\"white--text\">mdi-account</v-icon>\n            <v-toolbar-title class=\"font-weight-bold white--text\">\n              Register</v-toolbar-title\n            >\n            <v-spacer></v-spacer>\n            <v-btn text color=\"white\" class=\"ma-2 white--text font-weight-bold\" @click=\"tosignin\">\n              回到登录页面\n              <v-icon right dark>mdi-arrow-right-bold-outline</v-icon>\n            </v-btn>\n          </v-toolbar>\n            <v-stepper v-model=\"el\" vertical non-linear>\n            <v-stepper-step\n              :complete=\"el > 1\"\n              step=\"1\"\n              :rules=\"[verifystep1]\"\n              :color=\"complete[0]\"\n              edit-icon=\"mdi-check\"\n              editable\n            >\n              输入姓名,设置密码\n              <small>作为用户登录和找回密码的必要信息</small>\n            </v-stepper-step>\n            <v-stepper-content step=\"1\">\n              <v-card elevation=\"4\" outlined class=\"mb-6 pa-3\" fill-height>\n                <v-text-field\n                  light\n                  v-model=\"name\"\n                  :error-messages=\"nameErrors\"\n                  :counter=\"4\"\n                  prepend-icon=\"mdi-account\"\n                  label=\"姓名\"\n                  required\n                  outlined\n                  @input=\"$v.name.$touch()\"\n                  @blur=\"$v.name.$touch()\"\n                ></v-text-field>\n                <v-text-field\n                  v-model=\"password\"\n                  :error-messages=\"passwordErrors\"\n                  label=\"密码\"\n                  prepend-icon=\"mdi-form-textbox-password\"\n                  required\n                  :counter=\"10\"\n                  outlined\n                  @input=\"$v.password.$touch()\"\n                  @blur=\"$v.password.$touch()\"\n                ></v-text-field>\n                <v-text-field\n                  v-model=\"repeatpassword\"\n                  :error-messages=\"repeatpasswordErrors\"\n                  prepend-icon=\"mdi-onepassword\"\n                  label=\"重复密码\"\n                  required\n                  :counter=\"10\"\n                  outlined\n                  @input=\"$v.repeatpassword.$touch()\"\n                  @blur=\"$v.repeatpassword.$touch()\"\n                ></v-text-field>\n                <v-text-field\n                  v-model.number=\"phonenumber\"\n                  type=\"number\"\n                  :error-messages=\"phonenumberErrors\"\n                  label=\"电话号码\"\n                  prepend-icon=\"mdi-cellphone\"\n                  required\n                  outlined\n                  @input=\"$v.phonenumber.$touch()\"\n                  @blur=\"$v.phonenumber.$touch()\"\n                ></v-text-field>\n                <v-text-field\n                  v-model=\"email\"\n                  :error-messages=\"emailErrors\"\n                  label=\"邮箱\"\n                  prepend-icon=\"mdi-email\"\n                  required\n                  outlined\n                  @input=\"$v.email.$touch()\"\n                  @blur=\"$v.email.$touch()\"\n                ></v-text-field>\n                <v-text-field\n                  v-model=\"identity\"\n                  :error-messages=\"identityErrors\"\n                  label=\"身份证\"\n                  prepend-icon=\"mdi-identifier\"\n                  required\n                  outlined\n                  @input=\"$v.identity.$touch()\"\n                  @blur=\"$v.identity.$touch()\"\n                ></v-text-field>\n                <v-row class=\"pa-3\">\n                  <v-avatar class=\"mx-2\" color=\"indigo\">\n                    <img :src=\"avatar_url\" alt=\"John\" v-if=\"avatar_url!==''\" id=\"img\">\n                    <v-icon dark v-if=\"avatar_url==''\" @click=\"showavatar_object\">mdi-account-circle</v-icon>\n                  </v-avatar> \n                  <v-file-input\n                    d-inline\n                    :rules=\"avarules\"\n                    accept=\"image/png, image/jpeg, image/bmp\"\n                    placeholder=\"选个头像\"\n                    label=\"头像\" \n                    ref=\"img\"\n                    @change=\"getevent\"\n                  ></v-file-input>   \n                </v-row>\n              </v-card>\n              <v-btn color=\"primary font-weight-bold\" @click=\"el++\"\n                >下一步</v-btn>\n            </v-stepper-content>\n\n            <v-stepper-step\n              :complete=\"el > 2\"\n              step=\"2\"\n              :rules=\"[verifystep2]\"\n              :color=\"complete[1]\"\n              edit-icon=\"mdi-check\"\n              editable\n              >完善个人信息\n              <small>用于必要时候作为联系手段</small>\n              </v-stepper-step>\n            <v-stepper-content step=\"2\" >\n              <v-card elevation=\"4\" outlined class=\"mb-6 pa-3\" fill-height>\n                <v-radio-group v-model=\"gender\" row prepend-icon=\"fas fa-transgender\" mandatory>\n                  <div class=\"grey--text\"><strong h1>性别:</strong></div>\n                  <v-spacer hidden-md-up></v-spacer>\n                  <v-radio label=\"男\" value=\"男\"></v-radio>\n                  <v-radio label=\"女\" value=\"女\"></v-radio>\n                  <v-spacer></v-spacer>\n                </v-radio-group>\n                <v-radio-group v-model=\"merrage\" row prepend-icon=\"fas fa-venus-mars\" mandatory>\n                  <div class=\"grey--text\"><strong h1>婚姻关系:</strong></div>\n                  <v-spacer hidden-md-up></v-spacer>\n                  <v-radio label=\"已婚\" value=\"已婚\"></v-radio>\n                  <v-radio label=\"未婚\" value=\"未婚\"></v-radio>\n                  <v-spacer></v-spacer>\n                </v-radio-group>\n                <v-menu\n                  ref=\"menu\"\n                  v-model=\"menu\"\n                  :close-on-content-click=\"false\"\n                  transition=\"scale-transition\"\n                  offset-y\n                  min-width=\"290px\"\n                >\n                  <template v-slot:activator=\"{ on, attrs }\">\n                    <!-- v-slot:activator=\"{ on, attrs }\"是插槽，让子组件text的点击事件绑定在了v-menu上面 -->\n                    <v-text-field\n                      v-model=\"birthdate\"\n                      outlined\n                      required\n                      label=\"出生日期\"\n                      prepend-icon=\"mdi-calendar\"\n                      readonly\n                      v-bind=\"attrs\"\n                      v-on=\"on\"\n                    ></v-text-field>\n                  </template>\n                  <v-date-picker\n                    ref=\"picker\"\n                    v-model=\"birthdate\"\n                    elevation=\"24\"\n                    :max=\"new Date().toISOString().substr(0, 10)\"\n                    @change=\"save\"\n                  ></v-date-picker>\n                </v-menu>\n                <v-autocomplete\n                  v-model=\"native_place\"\n                  :items=\"states\"\n                  outlined\n                  :error-messages=\"native_placeErrors\"\n                  @input=\"$v.native_place.$touch()\"\n                  @blur=\"$v.native_place.$touch()\"\n                  label=\"籍贯\"\n                  prepend-icon=\"mdi-city\"\n                >\n                </v-autocomplete>\n                <v-autocomplete\n                  v-model=\"Folk\"\n                  :items=\"folk\"\n                  outlined\n                  :error-messages=\"FolkErrors\"\n                  @input=\"$v.Folk.$touch()\"\n                  @blur=\"$v.Folk.$touch()\"\n                  label=\"民族\"\n                  prepend-icon=\"mdi-account-supervisor-circle\"\n                >\n                </v-autocomplete>\n                <v-text-field\n                      v-model=\"living_address\"\n                      outlined\n                      label=\"住址\"\n                      :error-messages=\"living_addressErrors\"\n                      @input=\"$v.living_address.$touch()\"\n                      @blur=\"$v.living_address.$touch()\"\n                      prepend-icon=\"mdi-home-account\"\n                    ></v-text-field>\n                    <div class=\"grey--text\"><strong h1>添加紧急联系人\n                      <v-btn elevation=\"2\" fab icon outlined x-small @click=\"addrelable_person\"><v-icon>mdi-plus</v-icon></v-btn>\n                    </strong></div>\n                    <v-dialog v-model=\"dialog1\" width=\"500\" rounded-lg>\n                      <v-card>\n                        <v-card-title class=\"headline blue\">注意</v-card-title>\n                        <v-card-text class=\"grey--text\">最多添加三个联系人</v-card-text>\n                        <v-divider inset></v-divider>\n                        <v-card-actions>\n                          <v-spacer></v-spacer>\n                          <v-btn color=\"primary\" text @click=\"dialog1 = false\">知道了</v-btn>\n                        </v-card-actions>\n                      </v-card>\n                    </v-dialog>\n                    <v-row class=\"my-0\" v-for=\"(i,index) in relable_person\" :key=\"index\">\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.name\"\n                        dense\n                        outlined\n                        label=\"名字\"\n                        prepend-icon=\"fas fa-user-circle\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.relationship\"\n                        dense\n                        outlined\n                        label=\"关系\"\n                        prepend-icon=\"fas fa-user-friends\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model.number=\"i.phone_number\"\n                        dense\n                        outlined\n                        type=\"string\"\n                        label=\"电话\"\n                        prepend-icon=\"fas fa-mobile-alt\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-divider inset></v-divider>\n                    </v-row>\n              </v-card>\n              <v-btn color=\"primary font-weight-bold\" @click=\"el++\">下一步</v-btn>\n              <v-btn text color=\"primary font-weight-bold\" @click=\"el--\">返回</v-btn>\n            </v-stepper-content>\n\n            <v-stepper-step\n              :complete=\"el > 3\"\n              step=\"3\"\n              :rules=\"[verifystep3]\"\n              :color=\"complete[2]\"\n              edit-icon=\"mdi-check\"\n              editable\n              >填写履历\n              <small>了解过往工作经历</small>\n              </v-stepper-step>\n            <v-stepper-content step=\"3\">\n              <v-card elevation=\"4\" outlined class=\"mb-6 pa-3\" fill-height>\n                <div class=\"grey--text\"><strong h1>\n                  工作经历\n                  <v-btn elevation=\"2\" fab icon outlined x-small  @click=\"addemployments\"><v-icon>mdi-plus</v-icon></v-btn>\n                  </strong>\n                </div>\n                <v-dialog v-model=\"dialog2\" width=\"500\" rounded-lg>\n                      <v-card>\n                        <v-card-title class=\"headline blue\">注意</v-card-title>\n                        <v-card-text class=\"grey--text\">最多添加五个工作经历</v-card-text>\n                        <v-divider inset></v-divider>\n                        <v-card-actions>\n                          <v-spacer></v-spacer>\n                          <v-btn color=\"primary\" text @click=\"dialog2 = false\">知道了</v-btn>\n                        </v-card-actions>\n                      </v-card>\n                    </v-dialog>\n                <v-row class=\"my-0\" v-for=\"(i,index) in employments\" :key=\"index\">\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.company\"\n                        dense\n                        outlined\n                        label=\"公司\"\n                        prepend-icon=\"fas fa-building\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.job\"\n                        dense\n                        outlined\n                        label=\"职位\"\n                        prepend-icon=\"fas fa-clipboard\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.Start_stop_time\"\n                        dense\n                        outlined\n                        label=\"工作年限\"\n                        prepend-icon=\"fas fa-calendar-alt\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-divider inset></v-divider>\n                    </v-row>\n              </v-card>\n              <v-btn color=\"primary font-weight-bold\" @click=\"el++\">下一步</v-btn>\n              <v-btn text color=\"primary font-weight-bold\" @click=\"el--\">返回</v-btn>\n            </v-stepper-content>\n\n            <v-stepper-step\n              :complete=\"el > 4\"\n              step=\"4\"\n              :rules=\"[verifystep4]\"\n              :color=\"complete[3]\"\n              edit-icon=\"mdi-check\"\n              editable\n              >教育程度\n              <small>用以区别后期人员证照的报考门槛</small>\n              </v-stepper-step\n            >\n            <v-stepper-content step=\"4\">\n              <v-card class=\"mb-12 pa-3\" fill-height outlined elevation=\"4\">\n                <div class=\"grey--text\"><strong h1>添加教育经历\n                      <v-btn elevation=\"2\" fab icon outlined x-small @click=\"addeducations\"><v-icon>mdi-plus</v-icon></v-btn>\n                    </strong></div>\n                    <v-dialog v-model=\"dialog3\" width=\"500\" rounded-lg>\n                      <v-card>\n                        <v-card-title class=\"headline blue\">注意</v-card-title>\n                        <v-card-text class=\"grey--text\">最多添加三个教育经历</v-card-text>\n                        <v-divider inset></v-divider>\n                        <v-card-actions>\n                          <v-spacer></v-spacer>\n                          <v-btn color=\"primary\" text @click=\"dialog3 = false\">知道了</v-btn>\n                        </v-card-actions>\n                      </v-card>\n                    </v-dialog>\n                <v-row class=\"my-0\" v-for=\"(i,index) in educations\" :key=\"index\" wrap>\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.school\"\n                        dense\n                        outlined\n                        label=\"毕业学校\"\n                        prepend-icon=\"fas fa-university\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.major\"\n                        dense\n                        outlined\n                        label=\"专业\"\n                        prepend-icon=\"fas fa-book\"\n                        ></v-text-field>\n                        </v-col>\n                        <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                        <v-text-field\n                        v-model=\"i.education_level\"\n                        dense\n                        outlined\n                        label=\"教育程度\"\n                        prepend-icon=\"fas fa-glasses\"\n                        ></v-text-field>\n                        </v-col>\n                        <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                            <v-menu\n                            ref=\"menu2\"\n                            v-model=\"menucomplation.entrance_year\"\n                            :close-on-content-click=\"false\"\n                            transition=\"scale-transition\"\n                            offset-y\n                            min-width=\"290px\"\n                            >\n                        <template v-slot:activator=\"{ on, attrs }\">\n                        <v-text-field\n                        v-model=\"i.entrance_year\"\n                        dense\n                        outlined\n                        readonly\n                        label=\"入学时间\"\n                        prepend-icon=\"fas fa-calendar-alt\"\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                        ></v-text-field>\n                        </template>\n                        <v-date-picker\n                        ref=\"picker2\"\n                        v-model=\"i.entrance_year\"\n                        :max=\"new Date().toISOString().substr(0, 10)\"\n                        @change=\"save2\"\n                        ></v-date-picker>\n                        </v-menu>\n                        </v-col>\n\n                        <v-col cols=\"12\" xs=\"4\" md=\"4\" class=\"ligne\">\n                          <v-menu\n                            ref=\"menu3\"\n                            v-model=\"menucomplation.graduation_year\"\n                            :close-on-content-click=\"false\"\n                            transition=\"scale-transition\"\n                            offset-y\n                            min-width=\"290px\"\n                            >\n                        <template v-slot:activator=\"{ on, attrs }\">\n                        <v-text-field\n                        v-model=\"i.graduation_year\"\n                        dense\n                        outlined\n                        readonly\n                        label=\"毕业时间\"\n                        prepend-icon=\"fas fa-calendar-alt\"\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                        ></v-text-field>\n                        </template>\n                        <v-date-picker\n                        ref=\"picker3\"\n                        v-model=\"i.graduation_year\"\n                        :max=\"new Date().toISOString().substr(0, 10)\"\n                        @change=\"save3\"\n                        ></v-date-picker>\n                        </v-menu>\n                        </v-col>\n                    </v-row>\n                    <v-divider inset></v-divider>\n              </v-card>\n             <v-btn color=\"primary font-weight-bold\" @click=\"el++\">下一步</v-btn>\n              <v-btn text color=\"primary font-weight-bold\" @click=\"el--\">返回</v-btn>\n            </v-stepper-content>\n\n            <v-stepper-step\n              step=\"5\"\n              :rules=\"[verifystep5]\"\n              :color=\"complete[4]\"\n              edit-icon=\"mdi-check\"\n              editable\n              >自我介绍</v-stepper-step\n            >\n            <v-stepper-content step=\"5\">\n              <v-textarea class=\"mb-12 ma-4\" outlined\n              filled name=\"input-7-4\"\n              label=\"对岗位的理解和工作的期望\"\n              v-model=\"selfintro\"\n              ></v-textarea>\n              <v-dialog v-model=\"dialog4\" persistent max-width=\"290\">\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn color=\"primary font-weight-bold\" @click=\"console\" v-bind=\"attrs\"  v-on=\"on\"  :disabled=\"submited\" :loading=\"submited\">完成并提交</v-btn>\n                <v-dialog v-model=\"submited\" hide-overlay persistent width=\"300\">\n                  <v-card color=\"primary\" dark>\n                    <v-card-text>注册成功,正在创建个人资料\n                      <v-progress-linear indeterminate color=\"white\" class=\"mb-0\"></v-progress-linear>\n                    </v-card-text>\n                  </v-card>\n                </v-dialog>\n              </template>\n              <v-card>\n              <v-card-title class=\"headline \" color=\"primary\">确定是否要提交资料?</v-card-title>\n              <v-card-text>个人资料可以后期在个人设置中更改,可以在登录后完善.</v-card-text>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn color=\"red darken-1 white--text\" @click=\"dialog4 = false\">再改改</v-btn>\n                <v-btn color=\"blue darken-1 white--text\" @click=\"submit\">确定</v-btn>\n              </v-card-actions>\n              </v-card>\n            </v-dialog>\n              <v-btn text color=\"primary font-weight-bold\" @click=\"el--\"\n                >Cancel</v-btn>\n            </v-stepper-content>\n          </v-stepper>\n        </v-card>\n        <v-snackbar v-model=\"snackbar\">\n          {{snacktext}}\n          <template v-slot:action=\"{ attrs }\">\n            <v-btn color=\"red\" text v-bind=\"attrs\" @click=\"snackbar = false\">Close</v-btn>\n          </template>\n        </v-snackbar>\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n\n<script>\nimport { validationMixin } from \"vuelidate\";\nimport {required,maxLength,minLength,sameAs,email,} from \"vuelidate/lib/validators\";\nimport {\n  nameErrors,emailErrors,passwordErrors,repeatpasswordErrors,identityErrors,phonenumberErrors,\n  living_addressErrors,FolkErrors,native_placeErrors\n} from \"../computed/signup.js\";\nimport {\n  save,submit,\n  verifystep1,verifystep2,verifystep3,verifystep4,verifystep5,\n  addrelable_person,addemployments,addeducations,\n} from \"../method/signup.js\";\nimport { states, folk } from \"@/assets/china/provience.js\";\n\nexport default {\n  mixins: [validationMixin],\n  validations: {\n    name: { required, maxLength: maxLength(4) },\n    password: { required, minLength: minLength(3) },\n    repeatpassword: { required, sameAsPassword: sameAs(\"password\") },\n    phonenumber: { required },\n    identity: { required },\n    email: { required, email },\n    native_place: { required },\n    Folk: { required },\n    living_address: { required }\n  },\n  data() {\n    return {\n      name: \"\",\n      phonenumber:null,\n      password: \"\",\n      repeatpassword: \"\",\n      email: \"\",\n      identity:\"\",\n      identity_image:\"\",\n      birthdate: \"\",\n      native_place: \"\",\n      Folk: \"\",\n      merrage: \"\",\n      living_address: \"\",\n      gender:\"\",\n      relable_person:[{name:\"\",relationship:\"\",phone_number:\"\"}],\n      employments:[{Start_stop_time:\"\",company:\"\",job:\"\"}],\n      educations:[{school:\"\",major:\"\",education_level:\"\",entrance_year:\"\",graduation_year:\"\"}],\n      selfintro:\"\",\n      avatar_url:\"\",\n      avatar_object:null,\n\n      snacktext:'',\n      hasimage:false,\n      snackbar:false,\n      states,\n      folk,\n      menu: false,\n      menucomplation:{entrance_year:false,graduation_year:false},\n      dialog1: false,\n      dialog2: false,\n      dialog3: false,\n      dialog4: false,\n      submited:false,\n      complete: [\"blue\", \"blue\", \"blue\", \"blue\", \"blue\",\"blue\"],\n      el: 1,\n      avarules:[\n        value => !value || value.size < 10000000 || 'Avatar size should be less than 2 MB!',\n      ],\n    };\n  },\n  computed: {\n    nameErrors,\n    emailErrors,\n    passwordErrors,\n    repeatpasswordErrors,\n    identityErrors,\n    phonenumberErrors,\n    living_addressErrors,\n    FolkErrors,\n    native_placeErrors\n  },\n  methods: {\n    save,submit,\n    verifystep1,verifystep2,verifystep3,verifystep4,verifystep5,\n    addrelable_person,addemployments,addeducations,\n    console(){\n      console.log(\n        this.name,this.phonenumber,this.password,\n        this.repeatpassword,this.email,this.identity,\n        this.birthdate,this.native_place,this.Folk,\n        this.merrage,this.living_address,this.gender,\n        this.relable_person,this.employments,this.educations,this.selfintro,\n      )\n    },\n    save2(entrance_year) {\n    this.$refs.menu.save(entrance_year)\n    },\n    save3(graduation_year) {\n    this.$refs.menu.save(graduation_year)\n    },\n    tosignin(){\n        this.$router.push({path:'/signin'})\n    },\n    //选择本地图片并且显示出来\n    getevent(event){\n      if(event){\n      this.avatar_object = event\n      console.log(this.avatar_object)\n      var url = null;\n      if (window.createObjectURL != undefined){\n        url = window.createObjectURL(event)\n      }else if (window.webkitURL != undefined){\n        url = window.webkitURL.createObjectURL(event);\n      }else if(window.URL != undefined){\n        url = window.URL.createObjectURL(event);\n      }\n      console.log(url)\n      this.avatar_url=url\n      }else{this.avatar_url='',this.avatar_object=null}\n    },\n    showavatar_object(){\n      console.log(this.avatar_object)\n    }\n  },\n  watch: {\n    //日期选择器从年份选择开始\n    menu(val) {\n      val && setTimeout(() => (this.$refs.picker.activePicker = \"YEAR\"));\n    },\n    menu2(val) {\n      val && setTimeout(() => (this.$refs.picker.activePicker = \"YEAR\"));\n    },\n    menu3(val) {\n      val && setTimeout(() => (this.$refs.picker.activePicker = \"YEAR\"));\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/pages/login/components"}]}